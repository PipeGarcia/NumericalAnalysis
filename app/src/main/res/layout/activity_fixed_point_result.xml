<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="pipe.numericalanalisys.FixedPointResult"
    android:background="#89b6dd">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Output"
        android:id="@+id/textView48"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_gravity="center_horizontal"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="110dp"
        android:text="n"
        android:id="@+id/textView50"
        android:layout_column="0"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_below="@+id/textView48"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/textView53"
        android:layout_alignEnd="@+id/textView53" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="110dp"
        android:text="x"
        android:id="@+id/textView51"
        android:layout_column="0"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_below="@+id/textView50"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/textView50"
        android:layout_alignEnd="@+id/textView50" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="110dp"
        android:text="f(x)"
        android:id="@+id/textView52"
        android:layout_column="0"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_below="@+id/textView51"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/textView51"
        android:layout_alignEnd="@+id/textView51" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="110dp"
        android:text="error"
        android:id="@+id/textView53"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_column="0"
        android:layout_below="@+id/textView52"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listViewN"
        android:layout_below="@+id/textView48"
        android:layout_toRightOf="@+id/textView50"
        android:layout_toEndOf="@+id/textView50"
        android:layout_above="@+id/textView51" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listViewX"
        android:layout_alignTop="@+id/textView51"
        android:layout_alignLeft="@+id/listViewN"
        android:layout_alignStart="@+id/listViewN"
        android:layout_alignBottom="@+id/textView51" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listViewFX"
        android:layout_alignTop="@+id/textView52"
        android:layout_alignLeft="@+id/listViewX"
        android:layout_alignStart="@+id/listViewX"
        android:layout_above="@+id/textView53" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listViewERROR"
        android:layout_alignTop="@+id/textView53"
        android:layout_alignLeft="@+id/listViewFX"
        android:layout_alignStart="@+id/listViewFX"
        android:layout_alignBottom="@+id/textView53" />

</RelativeLayout>
