<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".BisectionResult"
    android:orientation="vertical"
    android:background="#89b6dd">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Output"
        android:id="@+id/textView8"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_gravity="center_horizontal"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="b"
        android:id="@+id/textView9"
        android:layout_column="0"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="f(x)"
        android:id="@+id/textView10"
        android:layout_column="0"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView9"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="n"
        android:id="@+id/textView11"
        android:layout_column="0"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="a"
        android:id="@+id/textView12"
        android:layout_column="0"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_below="@+id/textView11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="x"
        android:id="@+id/textView13"
        android:layout_column="0"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView10"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="error"
        android:id="@+id/textView26"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_column="0"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView13"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="f(a)"
        android:id="@+id/textView14"
        android:layout_column="0"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_below="@+id/textView12"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView"
        android:layout_below="@+id/textView8"
        android:layout_toRightOf="@+id/textView26"
        android:layout_above="@+id/textView12" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView2"
        android:layout_below="@+id/listView"
        android:layout_alignLeft="@+id/listView"
        android:layout_alignStart="@+id/listView"
        android:layout_alignBottom="@+id/textView12" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView3"
        android:layout_alignTop="@+id/textView14"
        android:layout_alignLeft="@+id/listView2"
        android:layout_alignStart="@+id/listView2"
        android:layout_alignBottom="@+id/textView14" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView4"
        android:layout_below="@+id/listView3"
        android:layout_alignLeft="@+id/listView3"
        android:layout_alignStart="@+id/listView3"
        android:layout_above="@+id/textView10" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView5"
        android:layout_below="@+id/listView4"
        android:layout_alignLeft="@+id/listView4"
        android:layout_alignStart="@+id/listView4"
        android:layout_alignBottom="@+id/textView10" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView6"
        android:layout_below="@+id/listView5"
        android:layout_toRightOf="@+id/textView26"
        android:layout_toEndOf="@+id/textView26"
        android:layout_above="@+id/textView26" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView7"
        android:layout_below="@+id/listView6"
        android:layout_toRightOf="@+id/textView26"
        android:layout_toEndOf="@+id/textView26"
        android:layout_alignBottom="@+id/textView26" />

</RelativeLayout>
