<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="pipe.numericalanalisys.NumericalIntegrationParameters">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Initial Parameters"
        android:id="@+id/textView229"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the initial x:"
        android:id="@+id/textView230"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_marginTop="49dp"
        android:layout_below="@+id/textView229"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: 0"
        android:ems="10"
        android:id="@+id/editText30"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/textView230"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the values of f(x):"
        android:id="@+id/textView231"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/editText37"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="30dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: 1;2.72"
        android:ems="10"
        android:id="@+id/editText36"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/textView231"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Continue"
        android:id="@+id/button10"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:layout_below="@+id/editText36" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the final x:"
        android:id="@+id/textView232"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/editText30"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="30dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: 1"
        android:ems="10"
        android:id="@+id/editText37"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/textView232"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
