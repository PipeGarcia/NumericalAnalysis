<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="pipe.numericalanalisys.NumericalIntegrationMethods">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Methods"
        android:id="@+id/textView233"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Trapeze"
        android:id="@+id/b_trapeze"
        android:layout_marginTop="80dp"
        android:layout_below="@+id/textView233"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Compound Trapeze"
        android:id="@+id/b_compound_trapeze"
        android:layout_alignTop="@+id/b_trapeze"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Simpson 1/3"
        android:id="@+id/b_simpson"
        android:layout_marginTop="101dp"
        android:layout_below="@+id/b_trapeze"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Compound Simpson 1/3"
        android:id="@+id/b_compound_simpson"
        android:layout_alignTop="@+id/b_simpson"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
