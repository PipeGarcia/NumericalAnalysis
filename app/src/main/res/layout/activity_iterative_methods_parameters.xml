<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="pipe.numericalanalisys.IterativeMethodsParameters">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Initial Parameters"
        android:id="@+id/textView192"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the matrix A:"
        android:id="@+id/textView193"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView192"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="3dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: 4 -2 1;-1 1 -3;10 3 1"
        android:ems="10"
        android:id="@+id/editText31"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/textView193"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the vector B:"
        android:id="@+id/textView194"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editText31"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="3dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: -3;4;0"
        android:ems="10"
        android:id="@+id/editText32"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/textView194"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the initial values of X:"
        android:id="@+id/textView195"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editText32"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="3dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: 0;0;0"
        android:ems="10"
        android:id="@+id/editText33"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/textView195"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the tolerance:"
        android:id="@+id/textView196"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editText33"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="3dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: 0.01"
        android:ems="10"
        android:id="@+id/editText34"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/textView196"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the delta:"
        android:id="@+id/textView197"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editText34"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="3dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: 0.001"
        android:ems="10"
        android:id="@+id/editText35"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/textView197"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the number of iterations:"
        android:id="@+id/textView185"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/editText35"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="3dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: 100"
        android:ems="10"
        android:id="@+id/editText25"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/textView185"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Generate"
        android:id="@+id/button7"
        android:background="#f123b7"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
