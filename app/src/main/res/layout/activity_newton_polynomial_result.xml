<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="pipe.numericalanalisys.NewtonPolynomialResult"
    android:background="#89b6dd">

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/scrollView11"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <HorizontalScrollView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/horizontalScrollView11" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Output"
                    android:id="@+id/textView213"
                    android:textSize="@dimen/abc_dialog_padding_material"
                    android:layout_gravity="center_horizontal"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="false" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="POINTS"
                    android:id="@+id/textView214"
                    android:layout_below="@+id/textView213"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:textSize="@dimen/abc_dialog_padding_top_material" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView215"
                    android:textSize="@dimen/abc_text_size_body_1_material"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/textView214"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="false" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="SOLUTION"
                    android:id="@+id/textView216"
                    android:textSize="@dimen/abc_dialog_padding_top_material"
                    android:layout_below="@+id/textView215"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="180dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView217"
                    android:textSize="@dimen/abc_text_size_body_1_material"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/textView216"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="false" />

            </RelativeLayout>
        </HorizontalScrollView>
    </ScrollView>
</RelativeLayout>
