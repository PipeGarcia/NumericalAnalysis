<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="pipe.numericalanalisys.GaussSeidelParameters">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Initial Parameters"
        android:id="@+id/textView168"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the matrix A:"
        android:id="@+id/textView169"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView168"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="3dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: 4 -2 1;-1 1 -3;10 3 1"
        android:ems="10"
        android:id="@+id/editText17"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/textView169"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the vector B:"
        android:id="@+id/textView170"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editText17"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="3dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: -3;4;0"
        android:ems="10"
        android:id="@+id/editText18"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/textView170"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the initial values of X:"
        android:id="@+id/textView171"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editText18"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="3dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: 0;0;0"
        android:ems="10"
        android:id="@+id/editText19"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/textView171"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the tolerance:"
        android:id="@+id/textView172"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editText19"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="3dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: 0.01"
        android:ems="10"
        android:id="@+id/editText20"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/textView172"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the delta:"
        android:id="@+id/textView173"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editText20"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="3dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: 0.001"
        android:ems="10"
        android:id="@+id/editText21"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/textView173"
        android:layout_centerHorizontal="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Generate"
        android:id="@+id/button5"
        android:background="#f123b7"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set the number of iterations:"
        android:id="@+id/textView174"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_below="@+id/editText21"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="3dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Ej: 100"
        android:ems="10"
        android:id="@+id/editText22"
        android:textSize="@dimen/abc_floating_window_z"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView174"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
